kind: AquaEnforcer
apiVersion: operator.aquasec.com/v1alpha1
metadata:
  name: {{ ocp4_workload_aquasec_namespace }}
  namespace: {{ ocp4_workload_aquasec_namespace }}
spec:
  infra:
    serviceAccount: {{ ocp4_workload_aquasec_csp_service_account }}
    version: '{{ ocp4_workload_aquasec_sub_channel }}'
  runAsNonRoot: false
  gateway:
    host: {{ ocp4_workload_aquasec_gateway_url }}
    port: 443
  token: {{ ocp4_workload_aquasec_enforcer_token }}